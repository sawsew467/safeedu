      "image": "./assets/adaptive-icon.png",
      "imageWidth": 150,
      "package": "com.vn.safeedu"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": false
